{
  "private": true,
  "name": "budget-frontend",
  "version": "1.0.0",
  "description": "React.js frontend for budget",
  "scripts": {
    "start": "npm run start-prod-server",
    "start-dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev-server\" \"npm run test:watch\"",
    "start-dev-server": "NODE_PATH=./src node bin/server/index.js",
    "start-prod-server": "NODE_PATH=./src node bin/server/index.js",
    "watch-client": "NODE_PATH=./src UV_THREADPOOL_SIZE=100 node webpack/webpack-dev-server.js",
    "build": "webpack --verbose --colors --display-error-details --config ./webpack/webpack.prod.config.js",
    "test": "NODE_PATH=./src mocha --compilers js:babel-core/register --require ./tests/test_helper.js --recursive ./tests/",
    "test:watch": "npm run test -- --watch"
  },
